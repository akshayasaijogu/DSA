class Solution{
    static double switchCase(int choice, List<Double> arr){
        // code here
        if(choice == 1){
            double R= arr.get(0);
            return Math.PI * R * R;
        }else if(choice == 2){
            double L = arr.get(0);
            double B = arr.get(1);
            return L * B;
        }
        return 0;
    }
}

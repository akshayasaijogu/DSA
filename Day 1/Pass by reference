class Solution {
    static int[] passedBy(int a, int b) {
        // code here
        a+=1;
        b+=2;
    
    return new int[]{a, b};
}
public static void main(String args[]){
Scanner sc = new Scanner(System.in);    
int a = sc.nextInt();
int b = sc.nextInt();
int[] res = passedBy(a,b);
       
        System.out.println(res);
    }
}

public class Solution {
    public static void nForest(int n) {
       
        if (n < 1 || n > 25) {
            System.out.println("N must be between 1 and 25.");
            return;
        }

        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                System.out.print("* ");
            }
            System.out.println();
        }
    }
}
